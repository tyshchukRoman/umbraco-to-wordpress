      <Value Culture="da-dk"><![CDATA[[{"key":"3eef4309-8a3d-411a-bb2c-985c1c27fbcf","name":"Header Text","ncContentTypeAlias":"headerText","PropType":null,"text":"Partnerskaber"},{"key":"305eb339-256a-4c41-8384-72b6f2b6faf9","name":"Image + Link","ncContentTypeAlias":"imagePlusLink","PropType":null,"image":"[\r\n  {\r\n    \"key\": \"fb20e9d6-cc1a-4fa2-88b5-fd145d78856a\",\r\n    \"mediaKey\": \"1477abcd-7d69-4c48-bae9-ffe3ba762930\"\r\n  }\r\n]","link":"[{\"name\":\"Parkeringsklagenævnet\",\"target\":\"_blank\",\"url\":\"https://www.parkeringsklagenaevnet.dk/\"}]"},{"key":"f0a0844d-3489-44c4-9c5d-b8ac4508cead","name":"Image + Link","ncContentTypeAlias":"imagePlusLink","PropType":null,"image":"[\r\n  {\r\n    \"key\": \"33fe5fb2-b295-4420-a5b0-79134506227b\",\r\n    \"mediaKey\": \"fa6c498d-91fd-4635-a67a-1f8b4188abdc\"\r\n  }\r\n]","link":"[{\"name\":\"Proptech\",\"target\":\"_blank\",\"url\":\"https://www.proptechdk.dk/\"}]"},{"key":"56485221-f96c-4a02-bbbb-13f022cecef1","name":"Image + Link","ncContentTypeAlias":"imagePlusLink","PropType":null,"image":"[\r\n  {\r\n    \"key\": \"4fb5b5b7-2ab9-4b63-bef6-898b5dee1e85\",\r\n    \"mediaKey\": \"6ccac973-3d39-408d-9a43-4383646c1bb8\"\r\n  }\r\n]","link":"[{\"name\":\"Byggesocietetet\",\"target\":\"_blank\",\"url\":\"https://www.byggesocietetet.dk/\"}]"},{"key":"0688b121-721b-46f4-894e-96a1cd948be6","name":"Image + Link","ncContentTypeAlias":"imagePlusLink","PropType":null,"image":"[\r\n  {\r\n    \"key\": \"22cfbe0f-abea-47e4-925b-5497e65ebc3e\",\r\n    \"mediaKey\": \"c85d0de9-65b1-4152-ab36-c1018998458c\"\r\n  }\r\n]","link":"[{\"name\":\"Estate media\",\"target\":\"_blank\",\"url\":\"https://estatemedia.dk/dk/\"}]"}]]]></Value>
